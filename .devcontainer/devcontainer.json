{
	"name": "Go",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"Postman.postman-for-vscode",
				"DavidAnson.vscode-markdownlint"
			]
		}
	},
	"forwardPorts": [
		8080
	],
	"postCreateCommand": "go mod download",
	"remoteUser": "root"
}
